#
# set some env variables for dev work
#

# colon-separated list of suffixes to ignore in tab completion
export FIGNORE=.svn

export MAVEN_HOME=$COMPONENTS_HOME/apache-maven-2.0.9
export JAVA_HOME_16=/System/Library/Frameworks/JavaVM.framework/Versions/1.6/Home
export JAVA_HOME=$JAVA_HOME_16
export JAVA_16_HOME=$JAVA_HOME_16

export SYKOPATH=$PATH
source .path